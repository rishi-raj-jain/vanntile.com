{
  "name": "vanntile-portfolio",
  "version": "0.9.0",
  "private": true,
  "dependencies": {
    "next": "latest",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@types/node": "^14.14.32",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "commitizen": "^4.2.3",
    "cz-emoji": "github:ngryman/cz-emoji",
    "husky": "^5.1.3",
    "lint-staged": "^10.5.4",
    "prettier": "^1.19.1",
    "standard-version": "^9.1.1",
    "typescript": "4.2"
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "commit": "cz",
    "release": "standard-version --no-verify --sign"
