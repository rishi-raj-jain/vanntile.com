  "private": true,
  "dependencies": {
    "next": "^10.0.8",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@types/eslint": "^7.2.7",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/node": "^14.14.32",
    "@types/prettier": "^2.2.2",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "autoprefixer": "^10.2.5",
    "commitizen": "^4.2.3",
    "cz-emoji": "github:ngryman/cz-emoji",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "postcss": "^8.2.8",
    "prettier": "^1.19.1",
    "standard-version": "^9.1.1",
    "tailwindcss": "^2.0.3",
    "typescript": "4.2"
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "commit": "cz",
    "release": "standard-version --no-verify --sign"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --staged --write",
      "eslint --cache . --fix"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": "14.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vanntile/vanntile.com.git"
  },
  "keywords": [],
  "author": "vanntile"
}
